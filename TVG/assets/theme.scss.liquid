/*---------------
Fade Animation
---------------*/
.fadein {
    position:absolute;
    top:125px;
    bottom: 0px;
    left:60px;
    margin:auto;
}
.fadein img {
    position:absolute;
    left:-65px;
    top:0;
    -webkit-animation-name: fade;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-duration: 16s;
    animation-name: fade;
    animation-iteration-count: infinite;
    animation-duration: 16s;
}

@-webkit-keyframes fade {
    0% {opacity: 0;}
    20% {opacity: 0;}
    33% {opacity: 1;}
    53% {opacity: 0;}
    100% {opacity: 0;}
}
@keyframes fade {
    0% {opacity: 0;}
    20% {opacity: 0;}
    33% {opacity: 1;}
    53% {opacity: 0;}
    100% {opacity: 0;}
}
